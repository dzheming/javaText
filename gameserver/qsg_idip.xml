  	<entry name="onlineCount" type="int32" desc="在线玩家数量" test="1" isverify="true" isnull="true"/>
  	<entry name="roleTotalCreate" type="int32" desc="在线玩家数量" test="1" isverify="true" isnull="true"/>
  </struct>
